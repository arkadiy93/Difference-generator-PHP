{
    "name": "arkadiy/php-project2",
    "description": "A command-line tool educational project",
    "keywords": ["gendiff project", "hexlet project"],
    "type": "library",
    "version": "0.1.3",
    "bin": [
        "bin/gendiff"
    ],
    "scripts": {
        "phpcs": "phpcs",
        "phpunit": "phpunit"
    },
    "autoload": {
        "files": [
          "src/index.php",
          "src/parsers.php",
          "src/renders/flatRender.php",
          "src/renders/deepRender.php",
          "src/renders/plainRender.php",
          "src/renders/jsonRender.php",
          "src/renders/index.php"
        ],
        "psr-4": {
            "Php\\Package\\Tests\\": "tests"
          }
    },
    "authors": [
        {
            "name": "Arkadiy Stepanov",
            "email": "arkadiy93@hotmail.com"
        }
    ],
    "require": {
        "docopt/docopt": "^1.0",
        "phpunit/phpunit": "^7.5",
        "funct/funct": "^1.5",
        "tightenco/collect": "^5.8",
        "symfony/yaml": "^4.3",
        "arkadiy/php-project2": "^0.1.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*"
    }
}
